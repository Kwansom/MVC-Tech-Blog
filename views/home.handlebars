<a href="/" class="home-link">
  <h1>Learning to code..</h1>
</a>

{{#if posts}}

  <ul class="ul-container">
    {{#each posts}}
      <li><a href="dashboard/post/{{id}}"><h2 id="post-title">{{title}}</h2></a>
        -
        <br />
        <strong>Posted by: {{User.username}}</strong>
        <br />
        <strong>Created at:</strong>
        {{createdAt}}</li>
    {{/each}}
  </ul>

{{else}}
  <p>No posts yet!</p>
{{/if}}

